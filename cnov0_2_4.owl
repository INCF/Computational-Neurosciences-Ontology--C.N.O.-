    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#cno_0000001"/>
          <owl:Class rdf:about="#cno_0000002"/>
          <owl:Class rdf:about="#cno_0000003"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasBiologicalCorrelate</rdfs:label>
    <rdfs:range rdf:resource="#cno_0000174"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="cno_0000205"/>
